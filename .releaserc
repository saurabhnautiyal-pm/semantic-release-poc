{
  "branches":[
     "main",
     "next"
  ],
  "plugins":[
     ["@semantic-release/commit-analyzer",
        {
           "preset":"angular",
           "parserOpts":{
              "noteKeywords":[
                 "BREAKING CHANGE",
                 "perf",
                 "feat",
                 "BREAKING CHANGES",
                 "BREAKING"
              ]
           }
        }
     ],
     ["@semantic-release/release-notes-generator",
        {
           "preset":"angular",
           "parserOpts":{
              "noteKeywords":[
                 "BREAKING CHANGE",
                 "perf",
                 "BREAKING CHANGES",
                 "BREAKING"
              ]
           },
           "writerOpts":{
              "commitsSort":[
                 "subject",
                 "scope"
              ]
           }
        }
     ],
     ["@semantic-release/changelog",
        {
           "changelogFile":"docs/CHANGELOG.md"
        }
     ],
     [ "@semantic-release/git",
        {
           "assets":[
              "docs/CHANGELOG.md"
           ]
        }
     ],
     "@semantic-release/github"
  ]
}